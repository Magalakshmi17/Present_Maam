<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.magalakshmi.present_maam">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".stafflogin"
            android:label="@string/title_activity_stafflogin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".studentlogin"
            android:label="@string/title_activity_studentlogin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Staffs"
            android:label="@string/title_activity_staffs"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Students"
            android:label="@string/title_activity_students"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Viewlist"
            android:label="@string/title_activity_viewlist"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>
